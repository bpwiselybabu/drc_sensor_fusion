<!--
s-model_articulated_macro(prefix)

Creates an articulated s-model hand with the passed in prefix.  If there
is only one hand in the urdf then an empty prefix can be used "".  If
there are multiple hands then a prefix followed by an "_" is needed.

-->
<robot xmlns:xacro="http://ros.org/wiki/xacro">
	
	<xacro:macro name="s-model_finger_articulated" params="prefix">
	    <gazebo reference="${prefix}link_0">
	        <mu1>11</mu1>
		<mu2>11</mu2>
	    </gazebo>
	    <gazebo reference="${prefix}link_1">
	        <mu1>11</mu1>
		<mu2>11</mu2>
	    </gazebo>
	    <gazebo reference="${prefix}link_2">
	        <mu1>11</mu1>
		<mu2>11</mu2>
	    </gazebo>
	    <gazebo reference="${prefix}link_3">
	        <mu1>11</mu1>
		<mu2>11</mu2>
	    </gazebo>


		<!-- link list -->
		<link name="${prefix}link_0">
			<visual>
				<origin xyz="0.020 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="$(find robotiq_s_model_visualization)/meshes/s-model_articulated/visual/link_0.STL" />
				</geometry>
				<material name="green">
					<color rgba="0 1 0 1"/>
				</material>
			</visual>
			<inertial>
        		        <mass value="0.02"/>
        			<origin xyz="0 0 0"/>
        			<inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
      			</inertial>

		
		</link>
		<link name="${prefix}link_1">
			<visual>
				<origin xyz="0.050 -.028 0" rpy="0 0 -0.52"/>
				<geometry>
				  <mesh filename="$(find robotiq_s_model_visualization)/meshes/s-model_articulated/visual/link_1.STL" />
				</geometry>
				<material name="green"/>
			</visual>
			<inertial>
        		        <mass value="0.02"/>
        			<origin xyz="0 0 0"/>
        			<inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
      			</inertial>
			<collision>
				   <origin rpy="0 0 -0.5" xyz="0.000 -0.015 0"/>
      <geometry>
        <box size="0.1 0.045 0.035"/> 
      </geometry>
				<material name="yellow">
					<color rgba="0 1 1 1"/>
				</material>
				
			</collision>
		</link>
		<link name="${prefix}link_2">
			<!--
			NOTE: This link has an offset in the z-direction based on the models that were
			used to generate grap-it poses.  With the offset below the model appears visually
			correct, but it is suspect.
			-->
			<visual>
				<origin xyz="0.039 0 0.0075" rpy="0 0 0"/>
				<geometry>
					<mesh filename="$(find robotiq_s_model_visualization)/meshes/s-model_articulated/visual/link_2.STL" />
				</geometry>
				<material name="green"/>
			</visual>
			<inertial>
        		        <mass value="0.02"/>
        			<origin xyz="0 0 0"/>
        			<inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
      			</inertial>
			<collision>
				    <origin rpy="0 0 0" xyz="0.02 -0.01 0"/>
				          <geometry>
					          <box size="0.05 0.04 0.035"/> 
					  </geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<link name="${prefix}link_3">
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0.52"/>
				<geometry>
					<mesh filename="$(find robotiq_s_model_visualization)/meshes/s-model_articulated/visual/link_3.STL" />
				</geometry>
				<material name="green"/>
			</visual>
			<inertial>
        		        <mass value="0.02"/>
        			<origin xyz="0 0 0"/>
        			<inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
      			</inertial>
			<collision>
				<origin rpy="0 0 0.5" xyz="0.018 0.003 0"/>
				<geometry>
					<box size="0.045 0.03 0.035"/> 
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>

		<!-- end of link list -->
		<!-- joint list -->
		
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}joint_1" type="revolute">
			<origin xyz="0.020 0 0" rpy="0 0 0" />
			<axis xyz="0 0 1" />
			<parent link="${prefix}link_0"/>
			<child link="${prefix}link_1"/>
        		<dynamics damping="0.1" friction="0" />
        		<limit effort="20" velocity="2" lower="-1.5" upper="1.5" />
        		<safety_controller k_position="100" k_velocity="100" soft_lower_limit="-10.6981" soft_upper_limit="10.6981" />
		</joint>
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}joint_2" type="fixed">
			<parent link="${prefix}link_1"/>
			<child link="${prefix}link_2"/>
			<axis xyz="0 0 1"/>
			<origin xyz="0.050 -.028 0" rpy="0 0 -0.52"/>
			<limit lower="0" upper="3.1416" effort="100" velocity="100"/>
		</joint>
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}joint_3" type="fixed">
			<parent link="${prefix}link_2"/>
			<child link="${prefix}link_3"/>
			<axis xyz="0 0 1"/>
			<origin xyz="0.039 0 0" rpy="0 0 0"/>
			<limit lower="0" upper="3.1416" effort="100" velocity="100"/>
		</joint>
		<!-- end of joint list -->
	</xacro:macro>
	
		<xacro:macro name="s-model_articulated" params="prefix">
	
	<xacro:s-model_finger_articulated prefix="${prefix}finger_1_"/>
	<xacro:s-model_finger_articulated prefix="${prefix}finger_2_"/>
	<xacro:s-model_finger_articulated prefix="${prefix}finger_middle_"/>
		<!-- link list -->
	        <gazebo reference="${prefix}palm">
		    <mu1>11</mu1>
		    <mu2>11</mu2>
		</gazebo>

		<link name="${prefix}palm">
			<visual>
				<geometry>
					<mesh filename="$(find robotiq_s_model_visualization)/meshes/s-model_articulated/visual/palm.STL" />
				</geometry>
				<material name="green">
					<color rgba="0 1 0 1"/>
				</material>
			</visual>
			<inertial>
        		        <mass value="2.0"/>
        			<origin xyz="0 0 0"/>
        			<inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
      			</inertial>

			<collision>
				  <origin rpy="0 0 0" xyz="0 -0.033 0"/>
      <geometry>
        <box size="0.13 0.17 0.13"/>         
      </geometry>
				<material name="yellow">
					<color rgba="0 1 1 1"/>
				</material>
			</collision>
					<collision>
				  <origin rpy="0 0 0" xyz="-0.085 -0.02 0"/>
      <geometry>
                <box size="0.08 0.08 0.07"/> 
      </geometry>
				<material name="yellow">
					<color rgba="0 1 1 1"/>
				</material>
			</collision>
		</link>
		<!-- end of link list -->
		<!-- joint list -->

		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}palm_finger_1_joint" type="fixed">
			<parent link="${prefix}palm"/>
			<child link="${prefix}finger_1_link_0"/>
			<axis xyz="0 1 0"/>
			<origin xyz="-0.0455 0.0214 0.036" rpy="0 3.1416 -1.57"/>
			<limit lower="-0.16" upper="0.25" effort="100" velocity="100"/>
		</joint>
		<!-- WARNING: LIMITS NOT CORRECT -->
		<joint name="${prefix}palm_finger_2_joint" type="fixed">
			<parent link="${prefix}palm"/>
			<child link="${prefix}finger_2_link_0"/>
			<axis xyz="0 1 0"/>
			<origin xyz="-0.0455 0.0214 -0.036" rpy="0 3.1416 -1.57"/>
			<limit lower="-0.25" upper="0.16" effort="100" velocity="100"/>
		</joint>
		<joint name="${prefix}palm_finger_middle_joint" type="fixed">
			<parent link="${prefix}palm"/>
			<child link="${prefix}finger_middle_link_0"/>
			<axis xyz="0 0 1"/>
			<origin xyz="0.0455 0.0214 0" rpy="0 0 1.57"/>
		</joint>
		<!-- end of joint list -->
	</xacro:macro>
</robot>
