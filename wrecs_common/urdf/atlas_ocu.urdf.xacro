<?xml version="1.0"?>
<robot name="atlas" xmlns:xacro="http://www.ros.org/wiki/xacro">

       
  <xacro:include filename="$(find wrecs_common)/urdf/atlas_skeleton_v3.urdf" />
  <xacro:include filename="$(find wrecs_common)/urdf/multisense_offline.urdf" />
      <xacro:include filename="$(find wrecs_common)/urdf/robotiq_hand_simplified.xacro" />
  
 <xacro:s-model_articulated prefix="r_"/>
   
   <joint name="r_hand_joint" type="fixed">
    <origin rpy="3.14 0 0" xyz="0 -0.215 0.013"/>
