        <child link="r_palm" />
        </joint>
        
    <xacro:s-model_articulated prefix="l_"/>
   
   <joint name="l_hand_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0.215 0.013"/>
        <parent link="l_hand" />
        <child link="l_palm" />
        </joint>
        
           
    <link name="l_tof_camera_frame">
        
     </link>
    <joint name="l_tof_joint" type="fixed">
    <origin rpy="-1.57 1.57 0" xyz="0 0.109 -0.010"/>
        <parent link="l_wrist" />
        <child link="l_tof_camera_frame" />
        </joint>     
 
</robot>
